<Window x:Class="KeyBoardTrainee.RegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyBoardTrainee"
        mc:Ignorable="d"
        Title="RegistrationWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TextBoxLogin" HorizontalAlignment="Left" Height="23" Margin="115,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="TextBoxPassword" HorizontalAlignment="Left" Height="23" Margin="630,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ButtonRegistration" Content="Registration" HorizontalAlignment="Left" Margin="350,300,0,0" VerticalAlignment="Top" Width="75" AutomationProperties.Name="buttonRegistration" Click="ButtonRegistration_Click" />

   
    </Grid>
</Window>
